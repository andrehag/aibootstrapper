{
  "name": "autobootstrapper",
  "version": "1.0.0",
  "description": "Automatically bootstrap a new project",
  "main": "lib/index.js",
  "author": "Andreas Hage",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "go": "node ./lib/index.js",
    "bgo": "yarn build && yarn go"
  },
  "dependencies": {
    "ink": "^4.1.0",
    "openai": "^3.2.1",
    "react": "^18.2.0",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "@types/ink": "^2.0.3",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.32"
  }
}
